<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.cardview.widget.CardView
        android:id="@+id/popularity_best_cd1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="18dp"
        app:cardCornerRadius="5dp">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/popularity_best_photo_iv1"
            android:layout_width="121dp"
            android:layout_height="121dp"
            android:background="@color/black"
            android:adjustViewBounds="true">
        </androidx.appcompat.widget.AppCompatImageView>

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="20dp"
            android:layout_height="20dp"
            app:srcCompat="@drawable/popularity_pop_photo_ranked"
            android:background="@drawable/popularity_best_ranking_round"
            android:adjustViewBounds="true">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/popularity_viewpager_ranking_tv1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            android:text="1"
            android:textColor="@color/white"
            android:textStyle="bold">
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.cardview.widget.CardView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/popularity_viewpager_tv1"
        android:layout_width="0dp"
        android:layout_height="26dp"
        app:layout_constraintTop_toBottomOf="@id/popularity_best_cd1"
        app:layout_constraintStart_toStartOf="@id/popularity_best_cd1"
        app:layout_constraintEnd_toEndOf="@id/popularity_best_cd1"
        android:layout_marginStart="1dp"
        android:textSize="9sp"
        android:layout_marginTop="6dp"
        android:text="Q4 유로탑 롤\n매트리스 2size"
        android:textColor="#838383">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/popularity_viewpager_tv2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/popularity_best_cd1"
        app:layout_constraintStart_toStartOf="@id/popularity_best_cd1"
        android:layout_marginStart="1dp"
        android:layout_marginTop="33dp"
        android:text="61%"
        android:textSize="9sp"
        android:textColor="#33C6F1"
        android:textStyle="bold"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/popularity_viewpager_tv3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/popularity_best_cd1"
        app:layout_constraintStart_toStartOf="@id/popularity_best_cd1"
        android:layout_marginStart="22.5dp"
        android:layout_marginTop="33dp"
        android:text="269,900"
        android:textSize="9sp"
        android:textColor="#000000"
        android:textStyle="bold"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.cardview.widget.CardView
        android:id="@+id/popularity_best_cd2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardCornerRadius="5dp">
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/popularity_best_photo_iv2"
            android:layout_width="121dp"
            android:layout_height="121dp"
