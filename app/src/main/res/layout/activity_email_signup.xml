<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    android:id="@+id/login_scroll_view"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".src.login.emailsignup.EmailSignup"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/login_email_signup_left_guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="17dp">
    </androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/login_email_signup_right_guide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="17dp">
    </androidx.constraintlayout.widget.Guideline>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/login_emailsignup_backbtn"
        android:layout_width="22dp"
        android:layout_height="19dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="16dp"
        android:layout_marginStart="20dp"
        android:background="@drawable/back_btn"
        >
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="12dp"
        android:text="@string/email_signup_title"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="15sp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="59dp"
        android:text="인테리어 전문가이시다면  "
        android:textSize="12sp"
        android:textStyle="bold"
        android:drawableEnd="@drawable/braket1">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/login_email_signup_email"
        android:layout_width="0dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="90dp"
        android:background="@drawable/login_email_login_border"
        android:hint="@string/email_signup_email"
        android:gravity="top"
        android:paddingStart="14dp"
        android:paddingTop="15dp"
        android:textSize="13sp"
        android:textCursorDrawable="@drawable/login_cursor_drawable"
        >
    </androidx.appcompat.widget.AppCompatEditText>
    //이메일 적합성 평가
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_email_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        app:layout_constraintBottom_toBottomOf="@id/login_email_signup_email"
        app:layout_constraintTop_toTopOf="@id/login_email_signup_email"
        android:layout_marginTop="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="10dp"
        android:text="@string/email_signup_email_warning"
        android:textColor="#FF5962"
        android:textSize="11sp"
        android:layout_marginEnd="10dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_verify"
        android:layout_width="0dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_email"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="14dp"
        android:background="@drawable/login_border_verify_inactivate"
        android:text="@string/email_signup_verify"
        android:gravity="center"
        android:textSize="13sp"
        android:textStyle="bold"
        android:textColor="#C0C7CA"
        android:clickable="false"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/login_email_signup_password"
        android:layout_width="0dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_verify"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="27dp"
        android:background="@drawable/login_email_login_border"
        android:hint="@string/email_signup_password"
        android:gravity="top"
        android:paddingStart="14dp"
        android:paddingTop="15dp"
        android:textSize="13sp"
        android:inputType="textPassword"
        android:textCursorDrawable="@drawable/login_cursor_drawable">
    </androidx.appcompat.widget.AppCompatEditText>
    //비밀번호 적합성 평가
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_password_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        app:layout_constraintBottom_toBottomOf="@id/login_email_signup_password"
        app:layout_constraintTop_toTopOf="@id/login_email_signup_password"
        android:layout_marginTop="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="10dp"
        android:text="@string/email_signup_password_warning"
        android:textColor="#FF5962"
        android:textSize="11sp"
        android:layout_marginEnd="10dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/login_email_signup_password_confirm"
        android:layout_width="0dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_password"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:background="@drawable/login_email_login_border"
        android:hint="@string/email_signup_password_confirm"
        android:gravity="top"
        android:paddingStart="14dp"
        android:paddingTop="15dp"
        android:textSize="13sp"
        android:inputType="textPassword"
        android:textCursorDrawable="@drawable/login_cursor_drawable">
    </androidx.appcompat.widget.AppCompatEditText>
    //비밀번호 확인 적합성 평가
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_password_confirm_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        app:layout_constraintBottom_toBottomOf="@id/login_email_signup_password_confirm"
        app:layout_constraintTop_toTopOf="@id/login_email_signup_password_confirm"
        android:layout_marginTop="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="10dp"
        android:text="@string/email_signup_password_confirm_warning"
        android:textColor="#FF5962"
        android:textSize="11sp"
        android:layout_marginEnd="10dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/login_email_signup_nickname"
        android:layout_width="0dp"
        android:layout_height="47dp"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_password_confirm"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="20dp"
        android:background="@drawable/login_email_login_border"
        android:hint="@string/email_signup_nickname"
        android:gravity="top"
        android:paddingStart="14dp"
        android:paddingTop="15dp"
        android:textSize="13sp"
        android:textCursorDrawable="@drawable/login_cursor_drawable">
    </androidx.appcompat.widget.AppCompatEditText>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_nickname_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        app:layout_constraintBottom_toBottomOf="@id/login_email_signup_nickname"
        app:layout_constraintTop_toTopOf="@id/login_email_signup_nickname"
        android:layout_marginTop="42dp"
        android:layout_marginStart="15dp"
        android:layout_marginBottom="10dp"
        android:text="@string/email_signup_nicknamewarning"
        android:textColor="#FF5962"
        android:textSize="11sp"
        android:layout_marginEnd="10dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <View
        android:id="@+id/login_terms_and_conditions"
        android:layout_width="0dp"
        android:layout_height="218dp"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_nickname_warning"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:background="@drawable/login_email_login_border"
        android:layout_marginTop="44dp">
    </View>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/login_terms_and_conditions"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        android:layout_margin="20dp"
        android:text="@string/email_signup_terms"
        android:textSize="13sp"
        android:textStyle="bold"
        android:textColor="@color/black">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/login_terms_and_conditions"
        app:layout_constraintEnd_toStartOf="@id/login_checkbox_all"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="10dp"
        android:text="@string/email_signup_agree_all"
        android:textSize="13sp"
        android:textStyle="bold"
        android:textColor="@color/black">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/login_checkbox_all"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:button ="@color/transparent"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/custom_checkbox"
        android:checked="false"
        android:clickable="true"
        app:layout_constraintTop_toTopOf="@id/login_terms_and_conditions"
        app:layout_constraintEnd_toEndOf="@id/login_terms_and_conditions">
    </androidx.appcompat.widget.AppCompatCheckBox>
    <View
        android:layout_width="340dp"
        android:layout_height="0.8dp"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        app:layout_constraintEnd_toEndOf="@id/login_terms_and_conditions"
        app:layout_constraintTop_toTopOf="@id/login_terms_and_conditions"
        android:layout_marginTop="52dp"
        android:background="#E3E3E3"
        >
    </View>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/term1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        app:layout_constraintTop_toTopOf="@id/login_terms_and_conditions"
        android:layout_marginTop="70dp"
        android:layout_marginStart="20dp"
        android:text="@string/email_signup_term1"
        android:textColor="@color/black"
        android:textSize="12sp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/term1"
        app:layout_constraintTop_toTopOf="@id/term1"
        app:layout_constraintBottom_toBottomOf="@id/term1"
        android:layout_marginStart="5dp"
        android:text="@string/email_signup_essential"
        android:textSize="9sp"
        android:textColor="#23BFE8"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/login_checkbox_1"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:button ="@color/transparent"
        android:background="@drawable/custom_checkbox"
        android:checked="false"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="@id/login_checkbox_all"
        app:layout_constraintTop_toTopOf="@id/term1"
        app:layout_constraintBottom_toBottomOf="@id/term1"
        app:layout_constraintEnd_toEndOf="@id/login_checkbox_all">
    </androidx.appcompat.widget.AppCompatCheckBox>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/term2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        app:layout_constraintTop_toBottomOf="@id/term1"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:text="@string/email_signup_term2"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:background="@drawable/login_border_underline"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/term2"
        app:layout_constraintTop_toTopOf="@id/term2"
        app:layout_constraintBottom_toBottomOf="@id/term2"
        android:layout_marginStart="5dp"
        android:text="@string/email_signup_essential"
        android:textSize="9sp"
        android:textColor="#23BFE8"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/login_checkbox_2"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:button ="@color/transparent"
        android:background="@drawable/custom_checkbox"
        android:checked="false"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="@id/login_checkbox_all"
        app:layout_constraintTop_toTopOf="@id/term2"
        app:layout_constraintBottom_toBottomOf="@id/term2"
        app:layout_constraintEnd_toEndOf="@id/login_checkbox_all">
    </androidx.appcompat.widget.AppCompatCheckBox>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/term3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        app:layout_constraintTop_toBottomOf="@id/term2"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:text="@string/email_signup_term3"
        android:textColor="@color/black"
        android:textSize="12sp"
        android:background="@drawable/login_border_underline"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/term3"
        app:layout_constraintTop_toTopOf="@id/term3"
        app:layout_constraintBottom_toBottomOf="@id/term3"
        android:layout_marginStart="5dp"
        android:text="@string/email_signup_essential"
        android:textSize="9sp"
        android:textColor="#23BFE8"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/login_checkbox_3"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:button ="@color/transparent"
        android:background="@drawable/custom_checkbox"
        android:checked="false"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="@id/login_checkbox_all"
        app:layout_constraintTop_toTopOf="@id/term3"
        app:layout_constraintBottom_toBottomOf="@id/term3"
        app:layout_constraintEnd_toEndOf="@id/login_checkbox_all">
    </androidx.appcompat.widget.AppCompatCheckBox>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/term4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_terms_and_conditions"
        app:layout_constraintTop_toBottomOf="@id/term3"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        android:text="@string/email_signup_term4"
        android:textColor="@color/black"
        android:textSize="12sp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/term4"
        app:layout_constraintTop_toTopOf="@id/term4"
        app:layout_constraintBottom_toBottomOf="@id/term4"
        android:layout_marginStart="5dp"
        android:text="@string/email_signup_choice"
        android:textSize="9sp"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatCheckBox
        android:id="@+id/login_checkbox_4"
        android:layout_width="21dp"
        android:layout_height="21dp"
        android:button ="@color/transparent"
        android:background="@drawable/custom_checkbox"
        android:checked="false"
        android:clickable="true"
        app:layout_constraintStart_toStartOf="@id/login_checkbox_all"
        app:layout_constraintTop_toTopOf="@id/term4"
        app:layout_constraintBottom_toBottomOf="@id/term4"
        app:layout_constraintEnd_toEndOf="@id/login_checkbox_all">
    </androidx.appcompat.widget.AppCompatCheckBox>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_email_signup_terms_warning"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        app:layout_constraintTop_toBottomOf="@id/login_terms_and_conditions"
        android:layout_marginTop="12dp"
        android:text="@string/email_signup_terms_warning"
        android:textColor="#FF5962"
        android:textSize="11sp"
        android:visibility="gone">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_recommend_tv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/login_email_signup_terms_warning"
        app:layout_constraintStart_toStartOf="@id/login_email_signup_left_guide"
        android:text="@string/email_signup_isrecommended"
        android:textSize="12sp"
        android:textColor="@color/black"
        android:layout_marginTop="28dp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/login_recommend_code"
        android:layout_width="0dp"
        android:layout_height="46dp"
        app:layout_constraintTop_toBottomOf="@id/login_recommend_tv"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="11dp"
        android:background="@drawable/login_email_login_border"
        android:hint="@string/email_signup_recommendcode"
        android:gravity="top|bottom"
        android:paddingStart="14dp"
        android:textSize="13sp"
        android:textCursorDrawable="@drawable/login_cursor_drawable">
    </androidx.appcompat.widget.AppCompatEditText>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="38dp"
        android:layout_height="31dp"
        app:layout_constraintTop_toTopOf="@id/login_recommend_code"
        app:layout_constraintEnd_toEndOf="@id/login_recommend_code"
        app:layout_constraintBottom_toBottomOf="@id/login_recommend_code"
        android:layout_marginEnd="13dp"
        android:text="@string/email_signup_confirm"
        android:gravity="center"
        android:textSize="12sp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:background="@drawable/login_border_signupfinish_off"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_signup_complete"
        android:layout_width="0dp"
        android:layout_height="49dp"
        app:layout_constraintTop_toBottomOf="@id/login_recommend_code"
        app:layout_constraintStart_toEndOf="@id/login_email_signup_left_guide"
        app:layout_constraintEnd_toStartOf="@id/login_email_signup_right_guide"
        android:layout_marginTop="29dp"
        android:background="@drawable/login_border_signupfinish_off"
        android:text="@string/email_signup_complete"
        android:gravity="center"
        android:textSize="13sp"
        android:textStyle="bold"
        android:textColor="#FFFFFF"
        android:clickable="true"
        >
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.legacy.widget.Space
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/login_signup_complete">
    </androidx.legacy.widget.Space>
</androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>