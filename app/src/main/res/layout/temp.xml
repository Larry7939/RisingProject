<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:hedgehog="http://schemas.android.com/tools">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/my_const1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="parent">
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="24.5dp"
        android:layout_height="24.5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="15dp"
        android:background="@drawable/home_top_basket_icon">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="24.5dp"
        android:layout_height="24.5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="58dp"
        android:src="@drawable/detail_bottom_bar_icon4">
    </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/my_setting"
            android:layout_width="26.5dp"
            android:layout_height="26.5dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="1dp"
            android:layout_marginEnd="95dp"
            android:src="@drawable/my_setting">
        </androidx.appcompat.widget.AppCompatImageView>
    </androidx.constraintlayout.widget.ConstraintLayout>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/my_tabs"
        android:layout_width="match_parent"
        android:layout_height="43dp"
        app:tabIndicatorHeight="3.5dp"
        app:tabIndicatorColor="#39C4EE"
        app:tabSelectedTextColor="#39C4EE"
        app:tabTextColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_const1"
        app:layout_constraintEnd_toEndOf="parent"
        android:paddingStart="10dp"
        android:paddingEnd="3dp"
        android:background="@drawable/home_custom_tab_border"
        style="@style/MyCustomTabLayout"
        app:tabRippleColor="@color/transparent"
        >
        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="프로필"
            />
        <com.google.android.material.tabs.TabItem
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="나의쇼핑" />

    </com.google.android.material.tabs.TabLayout>
    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/my_profile"
        android:layout_width="110dp"
        android:layout_height="110dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_tabs"
        android:layout_marginTop="30dp"
        android:layout_marginStart="30dp"
        app:srcCompat="@drawable/my_profile">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/my_profile"
        app:layout_constraintTop_toTopOf="@id/my_profile"
        android:layout_marginStart="20dp"
        android:layout_marginTop="10dp"
        android:text="Larry"
        android:textSize="26sp"
        android:textColor="@color/black"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/my_profile"
        app:layout_constraintTop_toTopOf="@id/my_profile"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        android:text="팔로워 0 | 팔로잉 0"
        android:textSize="12sp"
        android:textColor="#AFAFAF"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/my_profile"
        app:layout_constraintTop_toTopOf="@id/my_profile"
        android:layout_marginStart="30dp"
        android:layout_marginTop="75dp"
        android:text="친구 초대하고"
        android:textSize="12sp"
        android:textColor="#000000">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/my_profile"
        app:layout_constraintTop_toTopOf="@id/my_profile"
        android:layout_marginStart="104dp"
        android:layout_marginTop="75dp"
        android:text="5,000P"
        android:textSize="12sp"
        android:textColor="@color/appMainColor"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/my_profile"
        app:layout_constraintTop_toTopOf="@id/my_profile"
        android:layout_marginStart="145dp"
        android:layout_marginTop="75dp"
        android:text="받기"
        android:textSize="12sp"
        android:textColor="#000000">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="match_parent"
        android:layout_height="80dp"
        app:layout_constraintTop_toBottomOf="@id/my_tabs"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="170dp"

        android:weightSum="100"
        >
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/my_alram"
            android:adjustViewBounds="true"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            >
        </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/my_scrab"
            android:adjustViewBounds="true"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            >
        </androidx.appcompat.widget.AppCompatImageView>
        <androidx.appcompat.widget.AppCompatImageView
            android:layout_width="33dp"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/my_heart"
            android:adjustViewBounds="true"
            android:layout_gravity="center"
            android:layout_marginStart="80dp"
            >
        </androidx.appcompat.widget.AppCompatImageView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <androidx.appcompat.widget.LinearLayoutCompat
        android:id="@+id/my_linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/my_tabs"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="230dp">
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="알림"
            android:textSize="18sp"
            android:textColor="#7E7E7E"
            android:layout_marginStart="80dp">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="스크랩 0"
            android:textSize="18sp"
            android:textColor="#7E7E7E"
            android:layout_marginStart="65dp">
        </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="좋아요 0"
            android:textSize="18sp"
            android:textColor="#7E7E7E"
            android:layout_marginStart="50dp">
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.appcompat.widget.LinearLayoutCompat>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintTop_toBottomOf="@id/my_linear"
        android:layout_marginTop="20dp"
        android:background="#E6E6E6">
    </View>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_linear"
        android:layout_marginStart="15dp"
        android:layout_marginTop="40dp"
        android:text="나의 쇼핑"
        android:textColor="@color/black"
        android:textStyle="bold"
        android:textSize="19sp">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="12dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/my_linear"
        android:layout_marginEnd="15dp"
        android:layout_marginTop="45dp"
        app:srcCompat="@drawable/my_arrow"
        android:adjustViewBounds="true">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.cardview.widget.CardView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/my_linear"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="90dp"
        app:cardElevation="0dp">
    <androidx.appcompat.widget.AppCompatImageView
        android:layout_width="380dp"
        android:layout_height="90dp"
        android:background="#EDEDED">
    </androidx.appcompat.widget.AppCompatImageView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:layout_marginStart="70dp"
        android:text="0"
        android:textSize="19sp"
        android:textColor="@color/appMainColor"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginBottom="9dp"
        android:text="1"
        android:textSize="19sp"
        android:textColor="@color/appMainColor"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="end"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="70dp"
        android:text="0"
        android:textSize="19sp"
        android:textColor="@color/appMainColor"
        android:textStyle="bold">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="49dp"
        android:layout_marginStart="40dp"
        android:text="진행중 주문"
        android:textSize="15sp"
        android:textColor="@color/black">
    </androidx.appcompat.widget.AppCompatTextView>
    <androidx.appcompat.widget.AppCompatTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="49dp"
        android:layout_marginStart="167dp"
        android:text="내 쿠폰"
        android:textSize="15sp"
        android:textColor="@color/black">
    </androidx.appcompat.widget.AppCompatTextView>
        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="49dp"
            android:layout_marginStart="280dp"
            android:text="내 포인트"
            android:textSize="15sp"
            android:textColor="@color/black">
        </androidx.appcompat.widget.AppCompatTextView>
    </androidx.cardview.widget.CardView>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        app:layout_constraintTop_toBottomOf="@id/my_linear"
        android:layout_marginTop="220dp"
        android:background="#E6E6E6">
    </View>








</androidx.constraintlayout.widget.ConstraintLayout>


